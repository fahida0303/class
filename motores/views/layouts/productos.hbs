
<link rel="stylesheet" href="/styles.css">
    <h1 style="color: cornflowerblue;">Tabla de Productos</h1>

    <table style="justify-content: center;">
        <tr>
            <th>producto</th>
            <th>precio</th>
            <th>imagen</th>
        </tr>
        {{#each productos }}
        <tr>
            <td>{{this.producto}}</td>
            <td>{{this.precio}}</td>
            <td><img src="{{imagen}}" alt=""></td>
        </tr>
        {{/each}}
    </table>